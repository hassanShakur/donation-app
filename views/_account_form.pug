form#account-form
    .input-group
        label(for='fname') First Name
        input(type='text' name='fname' id='fname' value=loggedUser.fname)
    .input-group
        label(for='lname') Last Name
        input(type='text' name='lname' id='lname' value=loggedUser.lname)
    .input-group
        label(for='email') Email
        input(type='email' name='email' id='email' value=loggedUser.email)
    .input-group
        button(type='submit') Update